{
  "expo": {
    "name": "CertGamesApp",
    "slug": "CertGamesApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "certgamesapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Orbitron-Regular.ttf",
        "family": "Orbitron"
      },
      {
        "asset": "./assets/fonts/Orbitron-Bold.ttf",
        "family": "Orbitron-Bold"
      },
      {
        "asset": "./assets/fonts/Orbitron-Medium.ttf",
        "family": "Orbitron-Medium"
      },
      {
        "asset": "./assets/fonts/Orbitron-Black.ttf",
        "family": "Orbitron-Black"
      },
      {
        "asset": "./assets/fonts/ShareTechMono-Regular.ttf",
        "family": "ShareTechMono"
      },
      {
        "asset": "./assets/fonts/FiraCode-Regular.ttf",
        "family": "FiraCode"
      }
    ]
  }
}
